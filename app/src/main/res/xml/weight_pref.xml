<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <EditTextPreference
        android:key="max_weight_value"
        android:inputType="numberDecimal"
        android:title="Максимальный вес, кг">
    </EditTextPreference>

    <no.nordicsemi.android.sdr.preferences.DatePreference
        android:key="keyname"
        android:title="Title of the preference"
        android:defaultValue="2014-08-01"
        android:summary="Summary"/>

    <no.nordicsemi.android.sdr.preferences.TimePreference
        android:key="timeExport"
        android:title="time"
        android:defaultValue="23:55"
        android:summary="Summary"/>

    <EditTextPreference
        android:key="discrete_value"
        android:inputType="numberDecimal"
        android:title="Дискретность, кг">

    </EditTextPreference>


</PreferenceScreen>